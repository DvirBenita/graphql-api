{
  "name": "server",
  "version": "1.0.0",
  "description": "HTTP Server for handling requests",
  "main": "server.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "server": "nodemon ./src/server.js",
    "babel-node": "babel-node --presets=@babel/env",
