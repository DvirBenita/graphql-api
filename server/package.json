    "express": "^4.17.1",
    "graphql": "^15.3.0",
    "graphql-tools": "^6.0.14",
    "lodash": "^4.17.19",
    "morgan": "^1.10.0",
    "redis": "^3.0.2",
    "shortid": "^2.2.15"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/node": "^7.10.5",
    "@babel/plugin-transform-async-to-generator": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.10.5",
